{
    "require": {
        "php":"^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "tomasvotruba/unused-public": "^0.0.34",
        "tomasvotruba/type-coverage": "^0.0.9",
        "tomasvotruba/cognitive-complexity": "^0.0.5",
        "phpstan/extension-installer": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PortoMeetup\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
